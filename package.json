{
  "name": "taro-shmetro",
  "version": "1.0.0",
  "private": true,
  "description": "Taro Shmetro",
  "templateInfo": {
    "name": "react-native",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:weappprod": "npm run build:weapp -- --watch --env production",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch --qr",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "android": "react-native run-android --no-packager",
    "ios": "react-native run-ios --no-packager",
    "start": "npm run dev:rn --",
    "podInstall": "pod-install",
    "upgradePeerdeps": "install-peerdeps @tarojs/taro-rn@3.5.5 -o && install-peerdeps @tarojs/components-rn@3.5.5 -o && install-peerdeps @tarojs/router-rn@3.5.5 -o && pod-install",
    "lint": "eslint --cache src --fix",
    "format": "prettier --write src/**/*.{js,ts,tsx,scss}",
    "pretty-quick": "pretty-quick --staged",
    "prepare": "husky install",
    "test": "npm run pretty-quick"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "helloint",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/cameraroll": "^4.1.2",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/geolocation": "^2.1.0",
    "@react-native-community/netinfo": "^9.3.1",
    "@react-native-community/slider": "^4.2.4",
    "@react-native-masked-view/masked-view": "^0.2.7",
    "@react-native-picker/picker": "^2.4.4",
    "@reduxjs/toolkit": "^1.8.5",
    "@tarojs/components": "3.5.5",
    "@tarojs/react": "3.5.5",
    "@tarojs/runtime": "3.5.5",
    "@tarojs/taro": "3.5.5",
    "@tarojs/taro-rn": "3.5.5",
    "expo": "^46.0.10",
    "expo-av": "^11.2.3",
    "expo-barcode-scanner": "^11.4.0",
    "expo-brightness": "^10.3.0",
    "expo-camera": "^12.3.0",
    "expo-file-system": "^14.1.0",
    "expo-image-picker": "^13.3.1",
    "expo-keep-awake": "^10.2.0",
    "expo-location": "^14.3.0",
    "expo-modules-core": "^0.11.5",
    "expo-sensors": "^11.4.0",
    "i18next": "^21.9.2",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-http-backend": "^1.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.6",
    "react-native": "^0.68.2",
    "react-native-device-info": "^10.0.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-resizer": "^1.4.5",
    "react-native-pager-view": "^5.4.25",
    "react-native-safe-area-context": "^4.3.4",
    "react-native-screens": "^3.17.0",
    "react-native-svg": "^12.4.4",
    "react-native-syan-image-picker": "^0.5.3",
    "react-native-webview": "^11.23.1",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "taro-react-echarts": "^1.1.0",
    "taro-react-table": "^1.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@tarojs/cli": "3.5.5",
    "@tarojs/mini-runner": "3.5.5",
    "@tarojs/plugin-framework-react": "^3.5.5",
    "@tarojs/rn-runner": "3.5.5",
    "@tarojs/webpack-runner": "3.5.5",
    "@types/react": "^17.0.2",
    "@types/react-native": "^0.67.0",
    "@types/react-redux": "^7.1.24",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-preset-taro": "3.5.5",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-taro": "3.5.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.0",
    "install-peerdeps": "^3.0.3",
    "pod-install": "^0.1.23",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "react-refresh": "^0.14.0",
    "stylelint": "9.3.0",
    "typescript": "^4.1.0"
  },
  "resolutions": {
    "babel-plugin-minify-dead-code-elimination": "0.5.2",
    "@types/react": "17.0.45"
  }
}
